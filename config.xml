<?xml version='1.0' encoding='utf-8'?>
<widget id="com.minds.mobile" version="1.0.17" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Minds</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="permissions" value="none" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="auto-hide-splash-screen" value="false" />
    <preference name="AllowInlineMediaPlayback" value="true" />
    <access launch-external="yes" origin="mailto:*" />
    <platform name="ios">
        <icon height="180" src="res/ios/appicon-60@3x.png" width="180" />
        <icon height="60" src="res/ios/appicon-60.png" width="60" />
        <icon height="120" src="res/ios/appicon-60@2x.png" width="120" />
        <icon height="76" src="res/ios/appicon-76.png" width="76" />
        <icon height="152" src="res/ios/appicon-76@2x.png" width="152" />
        <icon height="40" src="res/ios/appicon-40.png" width="40" />
        <icon height="80" src="res/ios/appicon-40@2x.png" width="80" />
        <icon height="57" src="res/ios/appicon.png" width="57" />
        <icon height="114" src="res/ios/appicon@2x.png" width="114" />
        <icon height="72" src="res/ios/appicon-72.png" width="72" />
        <icon height="144" src="res/ios/appicon-72@2x.png" width="144" />
        <icon height="29" src="res/ios/appicon-small.png" width="29" />
        <icon height="58" src="res/ios/appicon-small@2x.png" width="58" />
        <icon height="50" src="res/ios/appicon-50.png" width="50" />
        <icon height="100" src="res/ios/appicon-50@2x.png" width="100" />
        <splash height="480" src="res/ios/Default.png" width="320" />
        <splash height="960" src="res/ios/Default@2x.png" width="640" />
        <splash height="1024" src="res/ios/Default-Portrait.png" width="768" />
        <splash height="2048" src="res/ios/Default-Portrait@2x.png" width="1536" />
        <splash height="1136" src="res/ios/Default-568h@2x.png" width="640" />
        <splash height="1334" src="res/ios/Default-667h.png" width="750" />
        <splash height="2208" src="res/ios/Default-Portrait-736h@3x.png" width="1242" />
    </platform>
    <platform name="android">
        <icon density="ldpi" src="res/android/dpi.png" />
        <icon density="mdpi" src="res/android/dpi.png" />
        <icon density="hdpi" src="res/android/dpi.png" />
        <icon density="xhdpi" src="res/android/dpi.png" />
        <splash density="port-hdpi" src="res/android/screen-dpi.png" />
        <splash density="port-ldpi" src="res/android/screen-dpi.png" />
        <splash density="port-mdpi" src="res/android/screen-dpi.png" />
        <splash density="port-xhdpi" src="res/android/screen-dpi.png" />
    </platform>
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="2750" />
    <feature name="StatusBar">
        <param name="ios-package" onload="true" value="CDVStatusBar" />
    </feature>
    <preference name="StatusBarStyle" value="blacktranslucent" />
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="Capture">
        <param name="android-package" value="org.apache.cordova.mediacapture.Capture" />
        <param name="ios-package" value="CDVCapture" />
    </feature>
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
        <param name="onload" value="true" />
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
    </feature>
    <feature name="FileTransfer">
        <param name="ios-package" value="CDVFileTransfer" />
        <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
    </feature>
    <feature name="Keyboard">
        <param name="ios-package" onload="true" value="IonicKeyboard" />
    </feature>
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="iosPersistentFileLocation" value="Library" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="xwalkVersion" value="14" />
    <access origin="*" />

</widget>
